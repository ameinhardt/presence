{
  "name": "@precense/server",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "Simple Server for testing",
  "author": "Axel Meinhardt <26243798+ameinhardt@users.noreply.github.com>",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./index.js",
  "bin": {
    "playground": "index.js"
  },
  "engines": {
    "node": ">=18.14.0",
    "npm": ">=8.5.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch src --exec \"tsc -b --incremental . && node -r source-map-support/register index ../renderer/dist \" -e ts",
    "dev:build": "cross-env NODE_ENV=production tsc -b",
    "build": "cross-env NODE_ENV=production rollup -c",
    "lint": "eslint --fix --ext .js,.json,.ts .",
    "precommit": "lint-staged",
    "start": "node index",
    "test": "jest"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "http-proxy": "^1.18.1",
    "https-proxy-agent": "5.0.1",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-compress": "^5.1.0",
    "koa-conditional-get": "^3.0.0",
    "koa-etag": "^4.0.0",
    "koa-helmet": "^6.1.0",
    "koa-proxies": "^0.12.3",
    "koa-send": "^5.0.1",
    "koa-static": "^5.0.0",
    "minimist": "^1.2.8",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@ameinhardt/eslint-config": "ameinhardt/eslint-config#semver:v0.1.3",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/http-proxy": "^1.17.10",
    "@types/jest": "^29.4.0",
    "@types/jest-json-schema": "^6.1.1",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-compose": "^3.2.5",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-conditional-get": "^2.0.0",
    "@types/koa-etag": "^3.0.0",
    "@types/koa-send": "^4.1.3",
    "@types/koa-static": "^4.0.2",
    "@types/koa__cors": "^3.3.1",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^18.15.1",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "jest-json-schema": "^6.1.0",
    "lint-staged": "^13.2.0",
    "nock": "^13.3.0",
    "nodemon": "^2.0.21",
    "rollup": "^3.19.1",
    "rollup-plugin-terser": "^7.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
